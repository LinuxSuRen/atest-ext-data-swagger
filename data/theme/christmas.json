{
  "color": {
    "--el-color-white": "#ffffff",
    "--el-color-black": "#000000",
    "--el-color-primary": "#2E8B57",
    "--el-color-primary-light-3": "#3DA76A",
    "--el-color-primary-light-5": "#5FBC85",
    "--el-color-primary-light-7": "#8BD1A8",
    "--el-color-primary-light-8": "#A8E0C0",
    "--el-color-primary-light-9": "#D5F2E1",
    "--el-color-primary-dark-2": "#1E6A41",
    "--el-color-success": "#228B22",
    "--el-color-success-light-3": "#3DA63D",
    "--el-color-success-light-5": "#5FBC5F",
    "--el-color-success-light-7": "#8BD18B",
    "--el-color-success-light-8": "#A8E0A8",
    "--el-color-success-light-9": "#D5F2D5",
    "--el-color-success-dark-2": "#1A691A",
    "--el-color-warning": "#DAA520",
    "--el-color-warning-light-3": "#E5B949",
    "--el-color-warning-light-5": "#EBCA77",
    "--el-color-warning-light-7": "#F2DEAB",
    "--el-color-warning-light-8": "#F7EBC8",
    "--el-color-warning-light-9": "#FCF7E9",
    "--el-color-warning-dark-2": "#B8860B",
    "--el-color-danger": "#B22222",
    "--el-color-danger-light-3": "#C44D4D",
    "--el-color-danger-light-5": "#D27979",
    "--el-color-danger-light-7": "#E3A9A9",
    "--el-color-danger-light-8": "#EDC5C5",
    "--el-color-danger-light-9": "#F9E7E7",
    "--el-color-danger-dark-2": "#8B0000",
    "--el-color-error": "#B22222",
    "--el-color-info": "#5D8AA8",
    "--el-color-info-light-3": "#7DA2B9",
    "--el-color-info-light-5": "#9DB7CA",
    "--el-color-info-light-7": "#C6D7E2",
    "--el-color-info-light-8": "#DAE5ED",
    "--el-color-info-light-9": "#EDF3F7",
    "--el-bg-color": "#FFFFFF",
    "--el-bg-color-page": "#FFF5F5",
    "--el-bg-color-overlay": "#FFFFFF",
    "--el-text-color-primary": "#2F4F4F",
    "--el-text-color-regular": "#556B2F",
    "--el-text-color-secondary": "#8B4513",
    "--el-text-color-placeholder": "#A9A9A9",
    "--el-text-color-disabled": "#C0C0C0",
    "--el-border-color": "#D2B48C",
    "--el-border-color-light": "#DEB887",
    "--el-border-color-lighter": "#E6D3B3",
    "--el-border-color-extra-light": "#F0EAD6",
    "--el-border-color-dark": "#A0522D",
    "--el-border-color-darker": "#8B4513",
    "--el-fill-color": "#FAF0E6",
    "--el-fill-color-light": "#FFF8DC",
    "--el-fill-color-lighter": "#FFFAF0",
    "--el-fill-color-extra-light": "#FFF5EE",
    "--el-fill-color-dark": "#F5DEB3",
    "--el-fill-color-darker": "#E6C9A8",
    "--el-fill-color-blank": "#FFFFFF",
    "--el-christmas-snowflake": "url('data:image/svg+xml;base64,')", // 雪花SVG
    "--el-christmas-pattern": "repeating-linear-gradient(45deg, transparent, transparent 10px, rgba(218,165,32,0.1) 10px, rgba(218,165,32,0.1) 20px)"  // 金色斜纹
  },
  "common": {
    "--el-border-width": "1px",
    "--el-border-style": "solid",
    "--el-border-color-hover": "var(--el-color-warning)",
    "--el-border": "var(--el-border-width) var(--el-border-style) var(--el-border-color)",
    "--el-svg-monochrome-grey": "#A9A9A9",
    "--el-border-radius-base": "4px",
    "--el-border-radius-small": "2px",
    "--el-border-radius-round": "20px",
    "--el-border-radius-circle": "100%",
    "--el-box-shadow": "0px 12px 32px 4px rgba(139, 69, 19, 0.1), 0px 8px 20px rgba(139, 69, 19, 0.08)",
    "--el-box-shadow-light": "0px 0px 12px rgba(178, 34, 34, 0.12)",
    "--el-box-shadow-lighter": "0px 0px 6px rgba(218, 165, 32, 0.12)",
    "--el-box-shadow-dark": "0px 16px 48px 16px rgba(46, 139, 87, 0.08), 0px 12px 32px rgba(46, 139, 87, 0.12), 0px 8px 16px -8px rgba(46, 139, 87, 0.16)",
    "--el-christmas-border": "1px solid var(--el-color-warning)"
  },
  "font": {
    "--el-font-size-extra-large": "20px",
    "--el-font-size-large": "18px",
    "--el-font-size-medium": "16px",
    "--el-font-size-base": "14px",
    "--el-font-size-small": "13px",
    "--el-font-size-extra-small": "12px",
    "--el-font-family": "'Mountains of Christmas', 'Helvetica Neue', Helvetica, 'PingFang SC', 'Hiragino Sans GB', 'Microsoft YaHei', '微软雅黑', Arial, sans-serif",
    "--el-font-weight-primary": "500",
    "--el-font-line-height-primary": "24px"
  },
  "size": {
    "--el-component-size-large": "40px",
    "--el-component-size": "32px",
    "--el-component-size-small": "24px"
  },
  "z-index": {
    "--el-index-normal": "1",
    "--el-index-top": "1000",
    "--el-index-popper": "2000"
  },
  "components": {
    "button": {
      "--el-button-font-weight": "600",
      "--el-button-border-color": "var(--el-border-color-dark)",
      "--el-button-bg-color": "var(--el-color-primary)",
      "--el-button-text-color": "#FFF8DC",
      "--el-button-disabled-text-color": "var(--el-text-color-disabled)",
      "--el-button-disabled-bg-color": "var(--el-fill-color-light)",
      "--el-button-disabled-border-color": "var(--el-border-color-light)",
      "--el-button-hover-text-color": "#FFFFFF",
      "--el-button-hover-bg-color": "var(--el-color-primary-dark-2)",
      "--el-button-hover-border-color": "var(--el-color-warning)",
      "--el-button-active-text-color": "var(--el-color-warning-light-9)",
      "--el-button-active-border-color": "var(--el-color-warning)",
      "--el-button-active-bg-color": "var(--el-color-primary-dark-2)",
      "--el-button-outline-color": "var(--el-color-warning-light-5)",
      "--el-button-bg-image": "var(--el-christmas-pattern)",
      "--el-button-hover-bg-image": "var(--el-christmas-snowflake), var(--el-christmas-pattern)",
      "--el-button-active-box-shadow": "0 0 0 2px var(--el-color-warning)",
      "--el-button-border-radius": "24px"
    },
    "input": {
      "--el-input-text-color": "var(--el-text-color-regular)",
      "--el-input-border": "var(--el-christmas-border)",
      "--el-input-hover-border": "1px solid var(--el-color-primary)",
      "--el-input-focus-border": "1px solid var(--el-color-warning)",
      "--el-input-transparent-border": "0 0 0 1px transparent inset",
      "--el-input-border-color": "var(--el-border-color-dark)",
      "--el-input-border-radius": "12px",
      "--el-input-bg-color": "var(--el-fill-color-extra-light)",
      "--el-input-icon-color": "var(--el-text-color-placeholder)",
      "--el-input-placeholder-color": "var(--el-color-info)",
      "--el-input-hover-border-color": "var(--el-color-primary)",
      "--el-input-clear-hover-color": "var(--el-color-danger)",
      "--el-input-focus-border-color": "var(--el-color-warning)",
      "--el-input-bg-image": "var(--el-christmas-snowflake)",
      "--el-input-bg-position": "98% center",
      "--el-input-bg-repeat": "no-repeat",
      "--el-input-bg-size": "16px 16px"
    },
    "tag": {
      "--el-tag-bg-color": "var(--el-color-danger-light-9)",
      "--el-tag-text-color": "var(--el-color-danger)",
      "--el-tag-border-color": "var(--el-color-danger-light-5)",
      "--el-tag-hover-color": "var(--el-color-danger-dark-2)",
      "--el-tag-bg-image": "var(--el-christmas-snowflake)",
      "--el-tag-bg-position": "95% center",
      "--el-tag-bg-size": "14px 14px",
      "--el-tag-bg-repeat": "no-repeat",
      "--el-tag-border-radius": "12px"
    },
    "notification": {
      "--el-notification-width": "330px",
      "--el-notification-padding": "14px 26px 14px 13px",
      "--el-notification-radius": "8px",
      "--el-notification-shadow": "var(--el-box-shadow-light)",
      "--el-notification-border-color": "var(--el-color-warning-light-5)",
      "--el-notification-icon-size": "24px",
      "--el-notification-close-font-size": "16px",
      "--el-notification-group-margin-left": "13px",
      "--el-notification-group-margin-right": "8px",
      "--el-notification-content-font-size": "13px",
      "--el-notification-content-color": "var(--el-text-color-regular)",
      "--el-notification-title-font-size": "16px",
      "--el-notification-title-color": "var(--el-color-danger)",
      "--el-notification-close-color": "var(--el-color-warning)",
      "--el-notification-close-hover-color": "var(--el-color-danger)",
      "--el-notification-border": "2px dashed var(--el-color-warning)",
      "--el-notification-bg-image": "var(--el-christmas-snowflake)",
      "--el-notification-bg-position": "right bottom",
      "--el-notification-bg-repeat": "no-repeat",
      "--el-notification-bg-size": "40px 40px"
    },
    "menu": {
      "--el-menu-bg-color": "var(--el-fill-color-light)",
      "--el-menu-hover-bg-color": "var(--el-color-primary-light-8)",
      "--el-menu-active-bg-color": "var(--el-color-primary-light-7)",
      "--el-menu-text-color": "var(--el-text-color-primary)",
      "--el-menu-active-color": "var(--el-color-danger)",
      "--el-menu-border-color": "var(--el-border-color-light)",
      "--el-menu-item-bg-image": "var(--el-christmas-snowflake)",
      "--el-menu-item-bg-position": "95% center",
      "--el-menu-item-bg-size": "12px 12px",
      "--el-menu-border-radius": "8px"
    },
    "card": {
      "--el-card-border-color": "var(--el-color-warning)",
      "--el-card-bg-color": "var(--el-fill-color-blank)",
      "--el-card-header-bg-color": "var(--el-color-primary-light-9)",
      "--el-card-border-radius": "12px",
      "--el-card-box-shadow": "var(--el-box-shadow-lighter)",
      "--el-card-pattern": "var(--el-christmas-pattern)"
    }
  }
}
